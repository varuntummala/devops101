pipeline {
    agent any
    tools {
        maven 'maven-3.3.9'
        jdk 'OpenJDK8'
    }
    
}
stage {
    stage('Initialize'){
        steps {
            echo "PATH = ${PATH}"
            echo "M2_HOME = ${M2_HOME}"
            sh 'mvn --version'
        }
    }
    stage('Build'){
        steps{
            echo 'This is a minimal pipeline'
        }
    }
}