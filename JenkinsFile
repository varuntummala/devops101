pipeline{
    agent any
    tools {
        maven 'mavaen-3.6.0'
        jdk 'Java9'
    }
    

stages{
    stage ('Initialize'){
        steps {
            echo "PATH = ${PATH}"
            echo "M2_HOME = ${M2_HOME}"
            sh 'mvn --version'
        }
    }
    stage ('Build'){
        steps{
            echo 'This is a minimal pipeline'
        }
    }
}
}